<?xml version="1.0" encoding="utf-8"?>
<ContextPublisherGrounding uri="http://ontology.igd.fhg.de/LightingServer.owl#LightingContextProvider" version="1.0.0" xmlns="http://universAAL.org/AndroidService#"> 
	<Actions>
		<Action androidAction="org.universaal.nativeandroid.lighting.LAMP_STATE_CHANGED" 
			    androidCategory="android.intent.category.DEFAULT">
				<Subject 	uri="http://ontology.universaal.org/Lighting.owl#LightSource"
							androidName="http://ontology.igd.fhg.de/LightingServer.owl#controlledLamp{lamp_number}"/>
				
				<Predicate 	uri="http://ontology.universaal.org/Lighting.owl#srcBrightness"/>				
				
				<Object androidExtraParameter="lamp_brightness" androidExtraParameterJavaClass="java.lang.Integer"/>			
		</Action>		
	</Actions>
</ContextPublisherGrounding>